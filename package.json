{
  "scripts": {
    "layer0:init": "layer0 init",
    "layer0:dev": "layer0 dev",
    "layer0:build": "npm run build:css && npm run build && layer0 build",
    "layer0:deploy": "npm run build:css && npm run build && layer0 deploy",
    "layer0:deploy-docs": "npm run build:css && npm run build && layer0 deploy -t=layer0-docs",
    "layer0:prod": "layer0 run --production",
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "webpack serve --content-base public",
    "watch:css": "postcss styles --base styles --dir src/styles -w",
    "build:css": "postcss styles --base styles --dir src/styles --env production"
  },
  "name": "layer0-svelte-example",
  "version": "1.0.0",
  "devDependencies": {
    "@layer0/cli": "^4.5.1",
    "@layer0/core": "^4.5.1",
    "@layer0/devtools": "^4.5.1",
    "@layer0/prefetch": "^4.5.1",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "mini-css-extract-plugin": "^1.3.4",
    "postcss": "^8.4.1",
    "postcss-cli": "^9.0.2",
    "svelte": "^3.31.2",
    "svelte-loader": "^3.0.0",
    "tailwindcss": "^2.2.19",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@layer0/svelte": "^4.5.1",
    "process": "^0.11.10",
    "register-service-worker": "^1.7.2",
    "svelte-navigator": "^3.1.5",
    "whatwg-fetch": "^3.6.2",
    "workbox-webpack-plugin": "^6.3.0"
  }
}
